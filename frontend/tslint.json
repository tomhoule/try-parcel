{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "interface-name": false,
      "max-classes-per-file": false,
      "no-shadowed-variable": false,
      "member-access": false,
      "ordered-imports": false,
      "quotemark": [true, "single"],
      "semicolon": [true, "never"]
    },
    "rulesDirectory": []
}
