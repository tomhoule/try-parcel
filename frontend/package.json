{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --project .",
    "build": "parcel build src/index.ts",
    "start": "parcel index.html",
    "test": "jest",
    "typecheck": "tsc --noEmit --watch",
    "watch-test": "jest --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "./src/setupTests.ts"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.1.0",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "parcel-bundler": "^1.6.0",
    "ts-jest": "^22.0.1",
    "ts-protoc-gen": "^0.4.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "typescript-fsa": "^2.5.0",
    "typescript-fsa-reducers": "^0.4.5"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "axios": "^0.17.1",
    "google-protobuf": "^3.5.0",
    "grpc-web-client": "^0.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.1"
  }
}
